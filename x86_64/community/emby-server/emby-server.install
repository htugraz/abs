post_install() {
    getent group emby > /dev/null 2>&1 || groupadd -g 422 emby
    getent passwd emby > /dev/null 2>&1 || useradd -c 'Emby Media Server' -u 422 -g emby -d /var/lib/emby -s /usr/bin/nologin emby
    passwd -l emby > /dev/null
}

post_upgrade() {
  post_install
}

